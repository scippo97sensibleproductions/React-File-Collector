{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "File Collector",
  "version": "1.0.0",
  "identifier": "com.scippo97productions.filecollector",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "title": "File Collector",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "publisher": "Scippo97 Sensible Productions",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 29
    },
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU5RjFDOTQ0NTIzRjM5MDQKUldRRU9UOVNSTW54NlpZTE1idktDVjhLWkJYSGQ0b1l6YzBpNmFJek1IWWl3RmplN0gwdkF1d2oK",
      "endpoints": [
        "https://github.com/scippo97sensibleproductions/FileCollectorReact/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}